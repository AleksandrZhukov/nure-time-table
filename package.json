{
  "name": "nure-time-table",
  "version": "0.0.0",
  "scripts": {
    "webpack:dev": "./node_modules/.bin/webpack-dev-server",
    "webpack:prod": "NODE_ENV=production webpack --progress --color",
    "dev": "nodemon -w server --exec \"babel-node server --presets es2015,stage-0\"",
    "server:build": "babel server -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run check_dep && npm run -s server:build",
    "check_dep": "./node_modules/.bin/check-dependencies --verbose"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.20.0",
    "body-parser": "~1.15.2",
    "debug": "~2.6.0",
    "encoding": "^0.1.12",
    "express": "~4.14.0",
    "iconv-lite": "^0.4.15",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.1",
    "react-redux-connector": "^1.0.0",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chalk": "^1.1.3",
    "check-dependencies": "^1.0.1",
    "cors": "^2.8.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.26.0",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.8",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "progress-bar-webpack-plugin": "^1.9.2",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "redux-logger": "^2.7.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2"
  }
}
